{
  "name": "Jira Details",
  "description": "Jira Details. Title, description, Copy to clipboard, Save as File",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  }
}
